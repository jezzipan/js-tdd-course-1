{
  "name": "spot-ifood",
  "version": "1.0.0",
  "description": "iFood Frontend Test",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jezzipan/ifood-frontend-test/issues"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Jessica Oliveira <jessica.leute@gmail.com> ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jezzipan/ifood-frontend-test/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.1",
    "fetch": "^1.1.0",
    "husky": "^0.11.9",
    "mocha": "^3.2.0",
    "node": "^14.13.0",
    "nyc": "^10.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^9.1.0",
    "sinon-chai": "^3.5.0",
    "sinon-stub-promise": "^4.0.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
